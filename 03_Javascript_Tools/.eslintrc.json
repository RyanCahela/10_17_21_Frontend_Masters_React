{
  "extends": ["eslint:recommended", "prettier"],
  "plugins": [],
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "env": {
    "es6": true, //allows for use of functions like map() and reduce
    "browser": true, //allows you to call fetch() and other global browser functions
    "node": true //allows you to use global and other node evironment values
  }
}
